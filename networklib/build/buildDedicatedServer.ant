<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="buildDedicatedServer" basedir="../../">

	<target name="buildDedicatedServer">
		<ant antfile="networklib/build/build.ant" useNativeBasedir="true" inheritAll="false" target="build" />

		<jar destfile="target/server/DedicatedJSettlersServer.jar">
			<manifest>
				<attribute name="Main-Class" value="networklib.server.DedicatedServerApp" />
				<attribute name="Class-Path" value="." />
			</manifest>
			<fileset dir="networklib/bin" />
		</jar>
	</target>
</project>
